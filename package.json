  "devDependencies": {
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.9.0",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "8.0.3",
    "nodemon": "3.0.1",
    "prettier": "3.0.0"
  }
